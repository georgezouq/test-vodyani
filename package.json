{
  "name": "@vodyani/vodyani",
  "license": "MIT",
  "version": "8.0.0",
  "author": "ChoGathK",
  "description": "ðŸš€ A fantastic solution for nodejs server-side apps.",
  "homepage": "https://github.com/vodyani/vodyani#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vodyani/vodyani.git"
  },
  "keywords": [
    "vodyani",
    "nodejs",
    "nestjs"
  ],
  "bugs": {
    "url": "https://github.com/vodyani/vodyani/issues"
  },
  "scripts": {
    "install:all": "npm i && npm run install:husky",
    "install:husky": "npx husky install",
    "start": "ts-node src/main",
    "build": "rm -rf dist && tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src && tsc",
    "lint:fix": "eslint --fix --ext .ts src/ test/",
    "lint:html-report": "eslint ./src -f html -o eslint-report.html",
    "lint:json-report": "eslint ./src -f json -o eslint-report.json"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": "@vodyani"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/test/.spec.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "./node_modules/(?!(lodash-es|other-es-lib))"
    ]
  },
  "dependencies": {
    "@nestjs/common": "8.4.4",
    "@nestjs/core": "8.4.4",
    "@nestjs/platform-express": "8.4.4",
    "@vodyani/ark": "^8.0.2",
    "@vodyani/core": "^8.0.1",
    "@vodyani/swagger": "^8.0.1",
    "@vodyani/winston": "^8.0.1",
    "@vodyani/transformer": "0.5.3",
    "@vodyani/validator": "0.13.0",
    "dayjs": "1.11.0",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "rxjs": "7.5.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@nestjs/testing": "8.4.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.180",
    "@types/module-alias": "2.0.1",
    "@types/node": "16.11.26",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.4",
    "@vodyani/eslint-config": "1.0.9",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  }
}
